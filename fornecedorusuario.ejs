            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/dashboard">iChose Management</a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse navbar-ex1-collapse">
          <ul class="nav navbar-nav side-nav">
           <li class="dropdown">
              <a href="" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-caret-square-o-down"></i> Dashboard <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="/mapa">...</a></li>
                  <li><a href="/ichose">...</a></li>
                  <li><a href="/interno">...</a></li>
                  <li><a href="/cadEvento">...</a></li>
                </ul>
            </li>

           <li class="dropdown">
              <a href="" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-home"></i> Provider <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="/fornecedorcadastro">        <i class="glyphicon glyphicon-pencil"></i> Fornecedor</a>     </li>
                  <li><a href="/fornecedorlocal">           <i class="glyphicon glyphicon-tower"> </i> Local</a>        </li> 
                  <li><a href="/fornecedorevento">          <i class="fa fa-beer">                </i> Evento</a>       </li>
                  <li><a href="/fornecedorusuario">         <i class="glyphicon glyphicon-user">  </i> Usuário</a>      </li>
                  <li><a href="/fornecedorconfiguracao">    <i class="glyphicon glyphicon-cog">   </i> Configuração</a> </li>
                </ul>
            </li>
              
            <li><a href="/grafico"><i class="fa fa-bar-chart-o"></i> ...</a></li>
            <li><a href="/relatorio"><i class="fa fa-table"></i> ...</a></li>
            <li><a href="/evento"><i class="fa fa-edit"></i> ...</a></li>
          </ul>

          <ul class="nav navbar-nav navbar-right navbar-user">
            <li class="dropdown user-dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> <%= user.local.email %> <b class="caret"></b></a>
              <ul class="dropdown-menu">
                <li><a href="/perfil"><i class="fa fa-user"></i> Perfil</a></li>
                <li><a href="/configuracao"><i class="fa fa-gear"></i> Configurações</a></li>
                <li class="divider"></li>
                <li><a href="/scrape"><i class="fa fa-gear"></i> Scraper</a></li>  
                <li class="divider"></li>
                <li><a href="/logout"><i class="fa fa-power-off"></i> Sair</a></li>
              </ul>
            </li>
          </ul>
        </div><!-- /.navbar-collapse -->
      </nav>

      <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb" id="glass">
                    <li class="active"><i class="glyphicon glyphicon-pencil"></i> Usuários</li>
                </ol>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-default">
                <!-- Default panel contents -->
                    <div class="panel-heading">Consulta</div>
                    <div class="panel-body">
                        <div class="input-group">
                            <input type         ="text" 
                                   class        ="form-control" 
                                   placeholder  ="digite aqui para filtrar..." 
                                   ng-model     ="filtro">
                            <div class="input-group-btn">
                                <button class       ="btn btn-success" 
                                        type        ="button" 
                                        data-toggle ="modal" 
                                        data-target="#myModal">
                                    <span class="glyphicon glyphicon-plus"></span>
                                    Novo</button>
                            </div><!-- /btn-group -->
                        </div><!-- /input-group -->
                    </div>
                    <div class="table-responsive"> 
                        <table class="table table-striped">
                        <thead>
                            <tr class="info">
                                <th><a href="" ng-click="predicate = 'nome'; reverse=!reverse">Nome</a></th>
                                <th><a href="" ng-click="predicate = 'email'; reverse=!reverse">E-mail</a></th>
                                <th><a href="" ng-click="predicate = 'cargo'; reverse=!reverse">Cargo</a>
                                <th><a href="" ng-click="predicate = 'datavalidade'; reverse=!reverse">Data Validade</a></th>
                                <th>Opções</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="registro in collections | filter:filtro | orderBy:predicate:reverse">
                                <td>{{registro.nome}}</td>
                                <td><a ng-href="mailto:{{registro.email}}" title={{registro.nome}} target="_blank">{{registro.email}}</a></td>
                                <td>{{registro.cargo}}</td>
                                <td>{{registro.datavalidade}}</td>
                                <td><a href="#" 
                                       ng-click="editar(registro)" 
                                       data-toggle="modal" 
                                       data-target="#myModal"> 
                                       <i class="glyphicon glyphicon-edit"></i>
                                    </a> | 
                                    <a href     ="#" 
                                       ng-hide  ="registro.forauso"
                                       ng-click ="erase(registro)">
                                       <i class="glyphicon glyphicon-remove"></i>
                                    </a></td>
                            </tr>
                        </tbody>
                        </table>
                    </div>
               </div> 
            </div>
            </div>
        </div><!-- /#page-wrapper -->
    </div><!-- /#wrapper -->

<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="myModalLabel">Informações do Usuário</h4>
      </div>
      <div class="modal-body">
          
        <!-- Nav tabs -->
        <ul class="nav nav-tabs" role="tablist">
          <li class="active">   <a href="#principal"    role="tab" data-toggle="tab">Principal</a></li>
<!--          <li>                  <a href="#endereco"     role="tab" data-toggle="tab" class="btnmap">Endereço</a></li>
          <li>                  <a href="#contato"      role="tab" data-toggle="tab" class="btnmap">Contato</a></li>
-->        </ul>
        
        <form>
        <!-- Tab panes -->
        <div class="tab-content">
            <div class="tab-pane active"  id="principal">
                <br>
                <input  type    ="hidden"
                        value   ="0"
                        ng-model="formData.id">
                <label>Nome</label> 
                <div class="input-group col-xs-18 col-md-12" autofocus>  
                    <input type         ="text" 
                           class        ="form-control" 
                           ng-model     ="formData.nome" 
                           placeholder  ="digite o nome completo sem abreviações..." 
                           autofocus>
                </div>
                <br>
                <label>Cargo</label>
                <div class="input-group col-xs-18 col-md-12">
                    <input type         ="text" 
                           class        ="form-control" 
                           ng-model     ="formData.cargo"
                           placeholder  ="digite o cargo exercido...">
                </div>
                <br>
                <label>E-mail</label>
                <div class="input-group col-xs-18 col-md-12">
                    <input type         ="email" 
                           class        ="form-control"
                           ng-model     ="formData.email"
                           placeholder  ="digite o e-mail para servir de usuário...">
                </div>
                <br>
                <div class="row">
                    <div class="col-md-6">
                    <label>Senha</label>
                    <div class="input-group col-xs-18 col-md-12">
                        <input type         ="text" 
                               class        ="form-control"
                               ng-model     ="formData.password"
                               name         ="ncnpj"
                               placeholder  ="Cadastro Nacional de Pessoa Jurídica"
                               onKeyPress   ="MascaraCNPJ(ncnpj);" 
                               maxlength    ="18" 
                               onBlur       ="ValidarCNPJ(ncnpj);">
                    </div>
                    </div>
                    <div class="col-md-6">
                    <label>Data de Validade</label>
                    <div class="input-group col-xs-18 col-md-12">
                        <input type         ="date" 
                               class        ="form-control"
                               ng-model     ="formData.inscricaoestadual"
                               placeholder  ="digite a inscrição estadual">
                    </div>
                    </div>
                </div>
                <br>
            </div>
<!--          <div class="tab-pane"         id="endereco">
              <br> 
              <label>Logradouro</label> 
              <div class="input-group col-xs-18 col-md-12" autofocus>  
                  <input type           ="text" 
                         class          ="form-control" 
                         ng-model       ="formData.logradouro"
                         placeholder    ="rua, avenida, servidão ..." 
                         autofocus>
              </div>
              <br>
              <label>Complemento</label> 
              <div class="input-group col-xs-18 col-md-12">  
                  <input type         ="text" 
                         class        ="form-control"
                         ng-model     ="formData.complemento"
                         placeholder  ="conjunto, condomínio, residencial ...">
              </div>
              <br>
              <div class="row">
                  <div class="col-md-6">
                        <label>Bairro</label> 
                        <div class="input-group col-xs-18 col-md-12">  
                            <input type         ="text" 
                                   class        ="form-control"
                                   ng-model     ="formData.bairro"
                                   placeholder  ="...">
                        </div>
                  </div>
                  <div class="col-md-6">
                        <label>Estado</label>
                        <div class="dropdown col-xs-18 col-md-12">
                          <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                              {{status.estado}}
                            <span class="caret"></span>
                          </button>
                          <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                            <li role="presentation" ng-repeat = "estado in estadoscidades" ng-click="uf(estado)"><a role="menuitem" tabindex="-1">{{estado.nome}}</a></li>
                          </ul>
                        </div>
                  </div>
              </div>
              <br>
              <div class="row">
                 <div class="col-md-6">
                        <label>Cidade</label>
                        <div class="dropdown col-xs-18 col-md-12">
                          <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                              {{status.cidade}}
                            <span class="caret"></span>
                          </button>
                          <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                            <li role="presentation" ng-repeat = "cidade in arrayscidades" ng-click="city(cidade)"><a role="menuitem" tabindex="-1">{{cidade}}</a></li>
                          </ul>
                        </div>
                </div>
                    <div class="col-md-6">
                        <label>CEP</label> 
                        <div class="input-group col-xs-18 col-md-12">  
                            <input type         ="text" 
                                   class        ="form-control" 
                                   name         ="vcep"
                                   ng-model     ="formData.cep"
                                   onKeyPress   ="MascaraCep(vcep);"
                                   maxlength    ="10" 
                                   onBlur       ="ValidaCep(vcep);">
                        </div>
                    </div>
              </div>
              <br>
              <div class="row">
                <div id="map_canvas"></div>
              </div>
          </div>
          <div class="tab-pane"         id="contato">
              <br> 
              <label>Nome de contato</label> 
              <div class="input-group col-xs-18 col-md-12" autofocus>  
                  <input type        ="text" 
                         class       ="form-control"
                         ng-model    ="formData.nomecontato"
                         placeholder ="nome do gerente, funcionário ou encarregado..." 
                         autofocus>
              </div>
              <br> 
              <div class="row">
                    <div class="col-md-6">
                        <label>Cargo</label> 
                        <div class="input-group col-xs-18 col-md-12">  
                            <input type         ="text" 
                                   class        ="form-control" 
                                   ng-model     ="formData.cargo"
                                   placeholder  ="...">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label>Setor</label> 
                        <div class="input-group col-xs-18 col-md-12">  
                            <input type         ="text" 
                                   class        ="form-control" 
                                   ng-model     ="formData.setor"
                                   placeholder  ="...">
                        </div>
                    </div>
              </div>
              <br> 
              <div class="row">
                    <div class="col-md-6">
                        <label>email</label>
                        <div class="input-group col-xs-18 col-md-12">
                            <input type         ="email" 
                                   class        ="form-control"
                                   ng-model     ="formData.email"
                                   placeholder  ="emaildecontato@empresa.com.br">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label>Telefone</label>
                        <div class="input-group col-xs-18 col-md-12">
                            <input  type         ="text" 
                                    class        ="form-control" 
                                    name         ="vtelefone"
                                    ng-model     ="formData.telefone"
                                    placeholder  ="telefone para contato..."
                                    onKeyPress   ="MascaraTelefone(vtelefone);" 
                                    maxlength    ="16"  
                                    onBlur       ="ValidaTelefone(vtelefone);">
                        </div>
                    </div>
              </div>
              <br> 
                <label>website</label>
                <div class="input-group col-xs-18 col-md-12">
                    <input type         ="url" 
                           class        ="form-control" 
                           ng-model     ="formData.website"
                           placeholder  ="http://...">
                </div>
          </div>
-->        </div>          
        </form>
          
      </div>
      <div class="modal-footer">
          <div class="col-lg-6" ng-show="formData.forauso">
            <div class="input-group">
                <span class="input-group-addon">
                    <input type         ="checkbox"
                           ng-click     ="formData.forauso=!formData.forauso"
                           ng-checked   ="formData.forauso"
                           ng-model     ="formData.forauso">
                </span>
                <input type         ="text" 
                       class        ="form-control" 
                       placeholder  ="Fora de uso">
            </div><!-- /input-group -->
        </div><!-- /.col-lg-6 -->
        <button type="button" class="btn btn-warning" data-dismiss="modal">Fechar</button>
        <button type="button" class="btn btn-success" data-dismiss="modal" ng-click="salvar()" >Salvar</button>
      </div>
    </div>
  </div>
</div>  
        
